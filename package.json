{
  "name": "node-shuttle",
  "version": "1.0.0",
  "private": "true",
  "description": "Example discord bot in Node.js to host on shuttle.rs",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
